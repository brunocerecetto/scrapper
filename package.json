{
  "name": "scrapper",
  "version": "1.0.0",
  "main": "dist/server.js",
  "repository": "https://github.com/brunocerecetto/scrapper.git",
  "author": "Bruno Cerecetto <bruno.cerecetto@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",
    "puppeteer": "^2.1.1",
    "ts-node": "^8.8.2",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.17",
    "@types/express": "^4.17.6",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build-ts": "tsc",
    "start": "yarn run serve",
    "serve": "yarn run build-ts && node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "echo \"Success: no test specified\" && exit 0",
    "watch": "npm run build-ts && concurrently 'npm:watch-ts' 'npm:watch-node'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "export CI=true && npm run build-ts && lint-staged && npm test",
      "pre-push": "export CI=true && npm run build-ts && lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint"
    ]
  }
}
